function resizeWindow(bChangeWidth)
{if(document.getElementById('body1').clientHeight>600)
winHeight=600;else
{try
{winHeight=document.getElementById('body1').clientHeight+MyWinHeight;if(navigator.appName!="Microsoft Internet Explorer"&&MyWinHeight==105)
winHeight+=198;}
catch(e)
{if(navigator.appName=="Opera")
winHeight=document.getElementById('body1').clientHeight-18;else
winHeight=document.getElementById('body1').clientHeight;}
if(navigator.userAgent.indexOf("Windows NT 5.1")!=-1)
winHeight+=10;}
if(document.body.clientWidth>489)
{if(navigator.appName=="Microsoft Internet Explorer")
if(navigator.userAgent.indexOf("MSIE 7")!=-1)
{if(document.body.clientWidth>590)
{window.resizeTo(document.getElementById('body1').clientWidth+30,winHeight+235);}
else
window.resizeTo(document.getElementById('body1').clientWidth+130,winHeight+140);}
else
{if(document.body.clientWidth>590)
window.resizeTo(document.getElementById('body1').clientWidth+5,winHeight-60);else
window.resizeTo(document.getElementById('body1').clientWidth+130,winHeight+50);}
else if(navigator.appName=="Netscape")
{window.resizeTo(document.getElementById('body1').clientWidth+130,winHeight+53);}
else if(navigator.appName=="Opera")
{window.resizeTo(document.getElementById('body1').clientWidth+130,winHeight+30);}
else
window.resizeTo(document.getElementById('body1').clientWidth+130,winHeight+30);}
else
{if(bChangeWidth==true)
{if(navigator.appName=="Netscape")
winHeight=winHeight+15;if(navigator.userAgent.indexOf("MSIE 7")!=-1)
winHeight=winHeight+25;try
{width=document.getElementById('body1').clientWidth+60+MyWinWidth;}
catch(e)
{if(navigator.appName=="Netscape"&&document.getElementById('body1').clientWidth==350)
{width=394;}
else if(navigator.appName=="Opera")
width=394;else
{if(navigator.appName=="Netscape")
width=document.getElementById('body1').clientWidth+10;else
{width=document.getElementById('body1').clientWidth+10;}}}}
else
width=document.getElementById('body1').clientWidth+6;if(navigator.appName=="Microsoft Internet Explorer")
{if(navigator.userAgent.indexOf("MSIE 7")!=-1)
{if(width==360)
window.resizeTo(width+35,winHeight+70);else if(width==444)
{window.resizeTo(width+35,winHeight+70);}
else
window.resizeTo(width+4,winHeight+72);}
else
{if(width==444)
window.resizeTo(width+35,winHeight+49);else
window.resizeTo(width,winHeight+45);}}
else if(navigator.appName=="Netscape")
window.resizeTo(width,winHeight+49);else if(navigator.appName=="Opera"&&winHeight==82)
window.resizeTo(width,winHeight+150);else if(navigator.appName=="Opera")
window.resizeTo(width,winHeight+60);else
window.resizeTo(width,winHeight+30);}}
