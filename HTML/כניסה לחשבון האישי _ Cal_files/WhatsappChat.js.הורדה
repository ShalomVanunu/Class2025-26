function onWhatsappClick(whatsappUrl) {
    identificationServiceRequestAndOpenWhatsappSupport(whatsappUrl);
}


function identificationServiceRequestAndOpenWhatsappSupport(whatsappUrl) {
    var url = "../Inbox/ServiceRequests.aspx/IdentificationServiceRequest";
    var args = '{}';

    var options =
    {
        type: "POST",
        cache: false,
        url: url,
        data: args,
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        async: false,
        timeout: 12000,
        error: function (p_response) {

        },
        success: function (p_response) {

        },
        beforeSend: function (request) {

        },
        complete: function (p_response) {
            navigateToWhatsapp(whatsappUrl);
        }
    };

    setTimeout(function () { jQuery.ajax(options); }, 0);
}

function navigateToWhatsapp(whatsappUrl) {
    var currHref = window.location.href.toLowerCase();

    //window.open('https://www.cal-online.co.il/service-and-support/whatsapp/');
    window.open(whatsappUrl);
    ga('send', 'event', { eventCategory: 'שירות צאט בוואטסאפ', eventAction: 'איזור אישי באתר הפעולות', eventLabel: currHref });
}