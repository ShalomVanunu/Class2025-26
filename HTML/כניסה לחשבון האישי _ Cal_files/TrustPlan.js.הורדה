function isCorrectIDNumber(source,arguments)
{try
{__defaultFired=false;}
catch(e){}
sum=0;mul=1;strId=arguments.Value;stringLen=strId.length;if(stringLen<6)
{arguments.IsValid=false;return false;}
for(i=0;i<stringLen;i++)
{digit=strId.substring(stringLen-i-1,stringLen-i);tproduct=parseInt(digit,10)*mul;if(tproduct>=10)
{sum+=(tproduct%10)+1;}
else
{sum+=tproduct;}
if(mul==1)
mul++;else
mul--;}
if(sum==0)
{arguments.IsValid=false;return false;}
if((sum%10)==0)
{arguments.IsValid=true;return true;}
else
{arguments.IsValid=false;return false;}}
