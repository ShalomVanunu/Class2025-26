function WriteOnlyNumeric(e)
{var realEvent=new TrueEvent(e);var keyPressed=realEvent.key;if(keyPressed==8||keyPressed==46||keyPressed==9||keyPressed==0||keyPressed==13||keyPressed==35||keyPressed==36)
{return true;}
if((keyPressed>=37)&&(keyPressed<=40))
{return false;}
if((keyPressed>=48)&&(keyPressed<=57))
{return true;}
stopReturnValue(e);return false;}
function stopReturnValue(e)
{if(e.preventDefault)
{e.preventDefault();}
else
{e.returnValue=false;}}
function TrueEvent(e)
{try
{var ie=window.event!=null&&navigator.userAgent.indexOf('MSIE')>0;var e=(ie)?window.event:e;this.sourceElement=(ie)?e.srcElement:e.target;this.targetElement=(ie)?e.toElement:e.relatedTarget;this.button=(ie)?e.button:e.which;this.key=(ie)?e.keyCode:e.which;this.shiftKey=e.shiftKey;this.type=e.type;this.posX=new MousePos(e).posX;this.posY=new MousePos(e).posY;}
catch(e){return null;}}
