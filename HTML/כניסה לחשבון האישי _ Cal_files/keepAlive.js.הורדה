function ajaxCallKeepAlive(ajaxUrl) {

    $.ajax
        ({
            type: "POST",
            url: ajaxUrl,
            data: {},
            contentType: "application/json; charset=utf-8",
            dataType: 'json',
            async: true,
            global: false,
            success: function (data) {
                console.log(JSON.stringify(data))
            },
            error: function (data) {
                console.log(JSON.stringify(data))
            }
        });
}

function keepAlive() {
    if (this.document.location.href.indexOf("Login") < 0) {
        const _url = "../Common/KeepAlive.asmx/KeepAliveWS";
        setTimeout(function () {
            ajaxCallKeepAlive(_url);
        }, 0)

    }
}
window.setInterval(function () {
    keepAlive();
}, 15000);

keepAlive();